    - 
      id: 10
      name: "add_objects.py"
      plugin: Python
      class: PythonScriptItem
      data: 
        file: "@JVRC_RTC_DIRECTORY@/launch/add_objects.py"
        executionOnLoading: true
        backgroundExecution: false
    -
      id: 11
      name: "ros_service_server.py"
      plugin: Python
      class: PythonScriptItem
      data:
        file: "@JVRC_RTC_DIRECTORY@/scripts/ros_service_server.py"
        executionOnLoading: true
